<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:App/etc/config.xsd">
    <default>
        <payment>
            <qwicpay>
                <active>1</active>
                <model>Qwicpay\Checkout\Model\QwicPay</model>
                <title>QwicPay</title>
                <order_status>processing</order_status>
                <is_gateway>1</is_gateway>
                <can_authorize>1</can_authorize>
                <can_capture>1</can_capture>
                <can_refund>0</can_refund>
                <can_use_checkout>1</can_use_checkout>
                <can_use_internal>0</can_use_internal>
                <can_use_for_multishipping>1</can_use_for_multishipping>
            </qwicpay>
            <qwicpay_one>
                <debug>1</debug>
                <active>1</active>
                <model>QwicpayOneGatewayFacade</model>
                <merchant_gateway_key backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <payment_action>initialize</payment_action>
                <title>QwicPay ONE</title>
                <currency>ZAR</currency>
                <can_initialize>1</can_initialize>
                <can_authorize>0</can_authorize>
                <can_capture>0</can_capture>
                <can_void>0</can_void>
                <can_use_checkout>1</can_use_checkout>
                <is_gateway>1</is_gateway>
                <sort_order>-10</sort_order>
            </qwicpay_one>
        </payment>
    </default>
</config>
